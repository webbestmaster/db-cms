{
  "name": "db-cms",
  "version": "0.0.1",
  "description": "db cms",
  "main": "",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "__start-web": "cross-env NODE_ENV=development webpack-cli serve --progress",
    "__start-web:hot": "cross-env NODE_ENV=development webpack-cli serve --progress --hot",
    "__build-web": "cross-env NODE_ENV=production webpack --progress",
    "start-server": "cross-env NODE_ENV=production babel-node -x .ts ./example/example.ts --trace-warnings",
    "start-site": "sudo nginx -c ~/user/master-git/db-cms/site/nginx/nginx.site.conf -g 'daemon off;'",
    "check": "npm run eslint && npm run stylelint && npm run tsc",
    "check:all": "npm run prettier; npm run eslint:fix; npm run stylelint:fix; npm run tsc",
    "check:fix": "npm run prettier && npm run eslint:fix && npm run stylelint:fix && npm run tsc",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**.scss",
    "stylelint:fix": "stylelint ./**.scss --fix",
    "prettier": "prettier --write \"./**/*.(js|ts|tsx)\"",
    "mdlint": "eslint --ext md .",
    "mdlint:fix": "eslint --ext md . --fix",
    "tsc": "tsc -p ./tsconfig.json",
    "ts-coverage": "typescript-coverage-report",
    "pre-commit-hook": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/db-cms.git"
  },
  "author": "",
  "license": "ISC",
